#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *next;
};

int isCircular(struct node *head) {
    if(!head) return 0;
    struct node *p = head->next;
    while(p && p != head) p = p->next;
    return p == head;
}

int main() {
    struct node *head = NULL, *p = NULL;
    int a[] = {2,4,6,7,5};
    for(int i = 0; i < 5; i++) {
        struct node *t = malloc(sizeof(struct node));
        t->data = a[i];
        t->next = NULL;
        if(!head) head = t;
        else p->next = t;
        p = t;
    }
    p->next = head;
    if(isCircular(head)) printf("True");
    else printf("False");
}
